{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "调试vs-in-Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8083/angular/src/index.html",
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}